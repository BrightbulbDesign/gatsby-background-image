{
  "name": "gatsby-background-image-es5",
  "version": "0.5.9",
  "description": "Lazy-loading React background-image component with optional support for the blur-up effect. Transpiled to ES5.",
  "keywords": [
    "gatsby",
    "gastby-component",
    "gatsby-image",
    "background-image",
    "backgroundImage",
    "background",
    "image",
    "react-component",
    "ES5"
  ],
  "author": "Tim Hagn <mail@timhagn.com>",
  "homepage": "https://github.com/timhagn/gatsby-background-image#readme",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.4",
    "global": "^4.3.2",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.0",
    "react-testing-library": "^5.9.0"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "main": "./index.js",
  "files": [
    "/index.js",
    "/index.d.ts",
    "/IntersectionObserverUtils.js",
    "/ImageUtils.js",
    "/BackgroundUtils.js",
    "/StyleUtils.js",
    "/HelperUtils.js"
  ],
  "repository": "https://github.com/timhagn/gatsby-background-image",
  "scripts": {
    "build": "npm run format && babel ../gatsby-background-image/src --out-dir . --ignore **/__tests__",
    "format": "prettier --write \"../gatsby-background-image/src/*.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w ../gatsby-background-image/src --out-dir . --ignore **/__tests__"
  },
  "bugs": {
    "url": "https://github.com/timhagn/gatsby-background-image/issues"
  }
}
